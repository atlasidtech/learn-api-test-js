{
  "name": "learn-api-test",
  "version": "1.0.0",
  "description": "starter repository to learn api test with javascript",
  "dependencies": {
    "chai": "4.2.0",
    "chai-json-schema": "^1.5.1",
    "dotenv": "^8.2.0",
    "install": "^0.13.0",
    "mocha": "8.1.0",
    "npm": "^6.14.8",
    "supertest": "4.0.2"
  },
  "scripts": {
    "test": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000",
    "test-tag": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep @regression",
    "test-tag-and": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep @smoke --grep @regression",
    "test-tag-or": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep '@put|get'",
    "test-data": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep @postdata",
    "test-deep": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep @deep",
    "test-before": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep @beforetest",
    "test-schema": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --grep @schema",
    "test-parallel": "mocha ./tests --recursive --reporter mochawesome --reporter-options reportDir=reports/mochawesome/ --timeout 30000 --parallel"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atlasidtech/learn-api-test-js.git"
  },
  "author": "atlas team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atlasidtech/learn-api-test-js/issues"
  },
  "homepage": "https://github.com/atlasidtech/learn-api-test-js#readme",
  "devDependencies": {
    "mochawesome": "^6.1.1"
  }
}
